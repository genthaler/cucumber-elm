{
  "name": "cucumber-elm",
  "description": "An implementation of Cucumber in Elm",
  "version": "0.0.1",
  "license": "BSD3",
  "author": "Gunther Enthaler <genthaler@users.noreply.github.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/genthaler/cucumber-elm"
  },
  "bugs": {
    "url": "https://github.com/genthaler/cucumber-elm/issues"
  },
  "bin": {
    "elm-cuke": "supervisor/supervisor.js"
  },
  "keywords": [
    "elm",
    "bdd",
    "spec",
    "cucumber",
    "gherkin",
    "cli"
  ],
  "private": true,
  "scripts": {
    "clean": "shx rm -rf node_modules && shx rm -rf elm-stuff && pushd supervisor && npm run clean && popd",
    "prepare": "elm-package install --yes && pushd supervisor && npm i",
    "pretest1": "elm-test",
    "test": "node supervisor/supervisor.js example/features/Cucumber.feature --glue-arguments-function example/glue/Glue.elm"
  },
  "dependencies": {
    "cucumber-elm-supervisor": "file:supervisor",
    "glob": "^7.1.2",
    "node-elm-compiler": "^4.5.0",
    "proxyquire": "^2.0.1",
    "rambda": "^1.1.5",
    "require-from-string": "^2.0.2",
    "shelljs": "^0.8.2"
  },
  "devDependencies": {
    "elm": "^0.18.0",
    "elm-test": "^0.18.12",
    "shx": "^0.3.2"
  }
}
