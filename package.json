{
  "name": "cucumber-elm",
  "description": "An implementation of Cucumber in Elm",
  "version": "0.0.1",
  "license": "BSD3",
  "author": "Gunther Enthaler <genthaler@users.noreply.github.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/genthaler/cucumber-elm"
  },
  "bugs": {
    "url": "https://github.com/genthaler/cucumber-elm/issues"
  },
  "bin": {
    "elm-cuke": "supervisor/supervisor.js"
  },
  "keywords": [
    "elm",
    "bdd",
    "spec",
    "cucumber",
    "gherkin",
    "cli"
  ],
  "private": true,
  "scripts": {
    "prepare": "elm make && pushd supervisor && npm i && popd && pushd example && npm i && popd",
    "pretest": "elm-test",
    "test": "node supervisor/supervisor.js example/features/Cucumber.feature --glue-arguments-function example/glue/StepDef.elm"
  },
  "dependencies": {
    "cucumber-elm-supervisor": "file:supervisor",
    "glob": "^7.1.3",
    "node-elm-compiler": "^4.5.0",
    "proxyquire": "^2.1.0",
    "rambda": "^1.2.2",
    "require-from-string": "^2.0.2",
    "shelljs": "^0.8.2"
  },
  "devDependencies": {
    "elm": "^0.19.0",
    "elm-github-install": "^1.6.1",
    "elm-test": "^0.19.0-beta8",
    "shx": "^0.3.2"
  }
}
